{
  "name": "wildfire-server",
  "cwd": "/root/git/wildfire",
  "script": "wildfire-server.js",
  "log_date_format": "YYYY-MM-DD HH:mm Z",
  "watch": true,
  "ignore_watch": [
    "[\\/\\\\]\\./",
    "node_modules",
    "public"
  ],
  "merge_logs": true,
  "watch_options": {
    "followSymlinks": false
  }
}
